ld pow(ld x, ll n) {
    ld ret=1;
    while (n--) ret*=x;
    return ret;
}

constexpr ld eps = 1e-8;
void solve(ll test) {
    ll i, j, k;
    ld p; cin>>p;

    ld min_val=1;
    ll ans=1;
    for (ll n=2;n<=16;n++) {
        ld P = pow(1-p, n);
        ld ET_n = P/n + (1-P);
        if (ET_n + eps < min_val) {
            min_val = ET_n;
            ans = n;
        }
        // cout<<n<<": "<<ET_n<<'\n';
    }
    cout<<ans<<'\n';
}

