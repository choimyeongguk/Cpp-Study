#include <bits/stdc++.h>
using namespace std;

int main() {
	ios_base::sync_with_stdio(0);
	cin.tie(0);
	cout.tie(0);
	
	string str;
	
	while(true) {
		getline(cin, str);
		if(str == "END") break;
		for(int i = str.size() - 1; i >= 0; i--) {
			cout << str[i];
		}
		cout << "\n";
	}
	
	return 0;
}