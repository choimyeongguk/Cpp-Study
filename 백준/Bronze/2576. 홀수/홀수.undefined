void setup() {
    if(!ndebug) {
        freopen("input.txt", "r", stdin);
        freopen("output.txt", "w", stdout);
    }
    else {
        ios_base::sync_with_stdio(0);
        cin.tie(0);
        cout.tie(0);
    }
}

void preprocess() {
    ll i, j, k;

}

void solve(ll testcase){
    ll i, j, k;
    ll Min = 1e10, Sum = 0;
    for (i=0; i<7; i++) {
        cin >> j;
        if (j & 1) {
            Min = min(Min, j);
            Sum += j;
        }
    }
    if (Sum) cout << Sum << "\n" << Min;
    else cout << -1;
}

int main() {
    setup();
    preprocess();
    ll t = 1;
    // cin >> t;
    for (ll testcase = 1; testcase <= t; testcase++){
        solve(testcase);
    }
    return 0;
}