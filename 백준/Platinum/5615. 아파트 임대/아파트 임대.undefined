using ull = uint64_t;
using u128 = unsigned __int128;

ull modmul(ull a, ull b, ull m) {
    return (u128)a * b % m;
}

ull modpow(ull a, ull e, ull m) {
    ull r = 1;
    a %= m;
    while (e) {
        if (e & 1) r = modmul(r, a, m);
        a = modmul(a, a, m);
        e >>= 1;
    }
    return r;
}

bool is_prime(ull n) {
    if (n<2 || n%2==0 || n%3==0) return n==2 || n==3;
    ull k = __builtin_ctzll(n-1), d = n-1 >> k;
    for (ull a: { 2, 325, 9375, 28178, 450775, 9780504, 1795265022 }) {
        ull p = modpow(a, d, n), i = k;
        if (!(a%n)) continue;
        while (p!=1 && p!=n-1 && i--) p = modmul(p, p, n);
        // 1이나 -1을 만나면 중단 -> 소수일 것 같음!
        // 1인 경우는 초기값에서 걸러지고, -1은 한 번이라도 등장하면 걸러짐
        if (p!=n-1 && i!=k) return false;
        // 소수일 가능성이 있는경우 (아니면 무조건 합성수)
        // case 1: 전부 1
        // case 2: 중간에 -1 나오고 그다음 전부 1
    }
    return true;
}

void solve(ll testcase){
    ll i, j, k;
    ll N; cin >> N;
    ll num, cnt = 0;
    while (N--) {
        cin >> num;
        cnt += is_prime(2*num + 1);
    }
    cout << cnt;
}

