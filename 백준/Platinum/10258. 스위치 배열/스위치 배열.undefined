void preprocess() {
    ll i, j, k;
}

void solve(ll test) {
    ll i, j, k;
    string s; cin>>s;
    ll n=s.size();
    ll ans=0;
    bool flag=false;
    for (i=0;i<n;i++) {
        ans<<=1;
        if (flag ^ (s[i]=='1')) ans|=1;
        if (s[i]=='1') flag^=1;
