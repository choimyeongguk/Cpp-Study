    cin>>cc; c.resize(cc);
    for (auto &x : c) cin>>x;
    for (auto &x: a) x += 30000;
    for (auto &x: b) x += 30000;
    for (auto &x: c) x += 30000;

    vl aaa(60001), ccc(60001);
    for (auto x: a) aaa[x] = 1;
    for (auto x: c) ccc[x] = 1;
    vl mv;
    mult(aaa, ccc, mv);
    ll ans = 0;
    for (auto &x: b) ans += mv[x*2];
    cout<<ans<<'\n';
}

