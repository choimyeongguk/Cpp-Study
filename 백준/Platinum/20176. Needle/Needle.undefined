void preprocess() {
    ll i, j, k;
}

constexpr ld pi = 3.1415926535897932384626L;
void fft(ll sign, ll n, vd &real, vd &imag) {
    ld theta = sign*2*pi/n;
    for (ll m=n;m>=2;m>>=1,theta*=2) {
        ld wr = 1, wi = 0;
        ld c = cos(theta), s = sin(theta);
        ll mh = m>>1;
        for (ll i=0;i<mh;i++) {
            for (ll j=i;j<n;j+=m) {
                ll k = j+mh;
                ld xr = real[j] - real[k];
                ld xi = imag[j] - imag[k];
                real[j] += real[k];
                imag[j] += imag[k];
                real[k] = wr*xr-wi*xi;
                imag[k] = wr*xi+wi*xr;
            }
            ld _wr = wr*c-wi*s;
            ld _wi = wr*s+wi*c;
            wr=_wr;
            wi=_wi;
        }
    }
    for (ll i=1, j=0;i<n;i++) {
        for (ll k=n>>1;k>(j^=k);k>>=1);
        if (j<i) {
            swap(real[i], real[j]);
            swap(imag[i], imag[j]);
        }
    }
}

void mult(vl &a, vl &b, vl &r) {
    ll n=a.size(), m=b.size();
    ll fn = 1;
    while (fn<n+m) fn<<=1;

    vd ra(fn), ia(fn), rb(fn), ib(fn);
    for (ll i=0;i<n;i++) ra[i] = a[i], ia[i] = 0;
    for (ll i=n;i<fn;i++) ra[i] = ia[i] = 0;
    for (ll i=0;i<m;i++) rb[i] = b[i], ib[i] = 0;
    for (ll i=m;i<fn;i++) rb[i] = ib[i] = 0;

    fft(1, fn, ra, ia);
    fft(1, fn, rb, ib);
    for (ll i=0;i<fn;i++) {
        ld real_part = ra[i]*rb[i] - ia[i]*ib[i];
        ld imag_part = ra[i]*ib[i] + rb[i]*ia[i];
        ra[i] = real_part;
        ia[i] = imag_part;
    }
    fft(-1, fn, ra, ia);

    r.assign(fn, 0);
    for (ll i=0;i<fn;i++) {
        r[i] = floor(ra[i]/fn+0.5L);
    }
    r.resize(a.size()+b.size()-1);
}

