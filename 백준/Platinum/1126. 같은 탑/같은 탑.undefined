    //     for (auto [k,v] : mp) {
    //         // 낮은 쪽에 더하는 경우
    //         ll l=v+block[i], h=v+k;
    //         if (l>h) swap(l, h);
    //         if (h-l <= remain)
    //             tmp[h-l] = max(tmp[h-l], l);
    //         // 높은 쪽에 더하는 경우
    //         l=v, h=v+k+block[i];
    //         if (l>h) swap(l, h);
    //         if (h-l <= remain)
    //             tmp[h-l] = max(tmp[h-l], l);
    //     }
    //     for (auto [k,v] : tmp)
    //         mp[k] = max(mp[k], v);
    // }
    // cout << (mp[0] ? mp[0] : -1);
}

int main() {
    setup();
    preprocess();
    ll t = 1;
    // cin >> t;
    for (ll testcase = 1; testcase <= t; testcase++){
        solve(testcase);
    }
    return 0;
}