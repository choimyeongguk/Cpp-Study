void preprocess() {
    ll i, j, k;

}

void solve(ll testcase){
    ll i, j, k;
    ll N; cin >> N;
    ll remain = 0;
    vl block(N);
    for (auto& e : block) {
        cin >> e;
        remain += e;
    }
    const ll MAX = 250'000;
    vl dp(MAX+1, -1), trans(MAX+1, -1);  // dp[i]=j := i차이를 만들 때, 낮은 건물의 최대값
    dp[0] = 0, trans[0] = 0;
    for (i=0; i<N; i++) {
        for (j=0; j<=MAX; j++) {
            if (dp[j] == -1) continue;
            // 낮은 건물에 더하는 경우
            ll l=dp[j]+block[i], h=dp[j]+j;
            if (l>h) swap(l, h);
            if (h-l<=MAX)
                trans[h-l] = max(trans[h-l], l);
            // 높은 건물에 더하는 경우
            l=dp[j], h=dp[j]+j+block[i];
            if (h-l<=MAX)
                trans[h-l] = max(trans[h-l], l);
        }
        for (j=0; j<=MAX; j++)
            dp[j] = trans[j];
    }
    cout << (dp[0] ? dp[0] : -1);

    // unordered_map<ll,ll> mp;    // mp[x]=y := x차이를 만들 때 낮은 건물의 최대값
